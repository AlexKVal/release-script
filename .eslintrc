{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "one-var": [2, { "initialized": "never" }],
    "no-console": 0,
    "no-eq-null": 0,
    "no-param-reassign": 0,
    "no-undef": 2,
    "no-use-before-define": 0
  }
}
