{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "one-var": [2, { "initialized": "never" }],
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-var": 2,
    "quotes": [2, "single", "avoid-escape"],
    "strict": 0
  }
}
